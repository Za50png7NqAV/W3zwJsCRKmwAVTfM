    <item>
      <title></title>
      <link>https://www.yojnaportal.com/download/generate/</link>
      <pubDate>Mon, 01 Jan 0001 00:00:00 +0000</pubDate>
      <guid>https://www.yojnaportal.com/download/generate/</guid>
      <description>&lt;!DOCTYPE html&gt;&#xA;&lt;html lang=&#34;en&#34;&gt;&#xA;&lt;head&gt;&#xA;    &lt;meta charset=&#34;UTF-8&#34; /&gt;&#xA;    &lt;meta name=&#34;viewport&#34; content=&#34;width=device-width, initial-scale=1.0&#34; /&gt;&#xA;    &lt;title&gt;Generate Permanent Link&lt;/title&gt;&#xA;&lt;/head&gt;&#xA;&lt;body&gt;&#xA;    &lt;h2&gt;Generate Permanent Link&lt;/h2&gt;&#xA;    &lt;input type=&#34;text&#34; id=&#34;inputUrl&#34; placeholder=&#34;Enter URL to encode&#34; style=&#34;width: 80%;&#34; /&gt;&#xA;    &lt;button onclick=&#34;generateLink()&#34;&gt;Generate Link&lt;/button&gt;&#xA;    &lt;p id=&#34;generatedLink&#34;&gt;&lt;/p&gt;&#xA;&#xA;    &lt;script&gt;&#xA;        // Predefined tokens (custom static tokens)&#xA;        const tokens = [&#xA;            &#34;abc123&#34;, &#34;xyz456&#34;, &#34;pqr789&#34;, &#xA;            &#34;rst111&#34;, &#34;mno222&#34;, &#34;uvw333&#34;&#xA;        ];&#xA;&#xA;        function generateLink() {&#xA;            const url = document.getElementById(&#39;inputUrl&#39;).value;&#xA;            if (url) {&#xA;                const encodedUrl = btoa(url); &#xA;                const token = tokens[Math.floor(Math.random() * tokens.length)];&#xA;&#xA;                permanentLinks[token] = encodedUrl;&#xA;&#xA;                const permanentLink = `https://yojnaportal.com/download/pdf?token=${token}&amp;url=${encodedUrl}`;&#xA;                &#xA;                document.getElementById(&#39;generatedLink&#39;).innerHTML = `&#xA;                    &lt;a href=&#34;${permanentLink}&#34; target=&#34;_blank&#34;&gt;${permanentLink}&lt;/a&gt;&#xA;                `;&#xA;            } else {&#xA;                alert(&#34;Please enter a valid URL&#34;);&#xA;            }&#xA;        }&#xA;&#xA;        // Store token and encoded URL mapping (to be accessed globally)&#xA;        const permanentLinks = {&#xA;            &#34;abc123&#34;: &#34;&#34;,&#xA;            &#34;xyz456&#34;: &#34;&#34;,&#xA;            &#34;pqr789&#34;: &#34;&#34;,&#xA;            &#34;rst111&#34;: &#34;&#34;,&#xA;            &#34;mno222&#34;: &#34;&#34;,&#xA;            &#34;uvw333&#34;: &#34;&#34;&#xA;        };&#xA;    &lt;/script&gt;&#xA;&lt;/body&gt;&#xA;&lt;/html&gt;</description>
    </item>
    <item>
      <title></title>
      <link>https://www.yojnaportal.com/download/loader/</link>
      <pubDate>Mon, 01 Jan 0001 00:00:00 +0000</pubDate>
      <guid>https://www.yojnaportal.com/download/loader/</guid>
      <description>&lt;!DOCTYPE html&gt;&#xA;&lt;html lang=&#34;en&#34;&gt;&#xA;&lt;head&gt;&#xA;    &lt;meta charset=&#34;UTF-8&#34;&gt;&#xA;    &lt;meta name=&#34;viewport&#34; content=&#34;width=device-width, initial-scale=1.0&#34;&gt;&#xA;    &lt;title&gt;Base64 URL Loader&lt;/title&gt;&#xA;    &lt;style&gt;&#xA;        body {&#xA;            font-family: Arial, sans-serif;&#xA;            text-align: center;&#xA;        }&#xA;        iframe {&#xA;            width: 100%;&#xA;            height: 500px;&#xA;            border: 1px solid #ccc;&#xA;            border-radius: 8px;&#xA;            margin-top: 20px;&#xA;        }&#xA;    &lt;/style&gt;&#xA;&lt;/head&gt;&#xA;&lt;body&gt;&#xA;&#xA;    &lt;h2&gt;Base64 URL Loader&lt;/h2&gt;&#xA;    &lt;div id=&#34;statusMessage&#34;&gt;Loading...&lt;/div&gt;&#xA;    &lt;iframe id=&#34;pdfFrame&#34; src=&#34;&#34;&gt;&lt;/iframe&gt;&#xA;&#xA;    &lt;script&gt;&#xA;        window.onload = function() {&#xA;            const urlParams = new URLSearchParams(window.location.search);&#xA;            const encodedUrl = urlParams.get(&#39;url&#39;);&#xA;&#xA;            if (encodedUrl) {&#xA;                try {&#xA;                    const decodedUrl = atob(encodedUrl);&#xA;                    document.getElementById(&#39;pdfFrame&#39;).src = decodedUrl;&#xA;                    document.getElementById(&#39;statusMessage&#39;).innerText = &#34;PDF Loaded Successfully&#34;;&#xA;                } catch (e) {&#xA;                    document.getElementById(&#39;statusMessage&#39;).innerText = &#34;Invalid Base64 Format&#34;;&#xA;                }&#xA;            } else {&#xA;                document.getElementById(&#39;statusMessage&#39;).innerText = &#34;No URL Provided&#34;;&#xA;            }&#xA;        };&#xA;    &lt;/script&gt;&#xA;&lt;/body&gt;&#xA;&lt;/html&gt;</description>
    </item>
    <item>
      <title></title>
      <link>https://www.yojnaportal.com/download/pdf/</link>
      <pubDate>Mon, 01 Jan 0001 00:00:00 +0000</pubDate>
      <guid>https://www.yojnaportal.com/download/pdf/</guid>
      <description>&lt;!DOCTYPE html&gt;&#xA;&lt;html lang=&#34;en&#34;&gt;&#xA;&lt;head&gt;&#xA;    &lt;meta charset=&#34;UTF-8&#34; /&gt;&#xA;    &lt;meta name=&#34;viewport&#34; content=&#34;width=device-width, initial-scale=1.0&#34; /&gt;&#xA;    &lt;title&gt;Redirecting...&lt;/title&gt;&#xA;&lt;/head&gt;&#xA;&lt;body&gt;&#xA;    &lt;script&gt;&#xA;        // Predefined token and encoded URL mapping (same as in generate.html)&#xA;        const permanentLinks = {&#xA;            &#34;abc123&#34;: &#34;&#34;, &#xA;            &#34;xyz456&#34;: &#34;&#34;,&#xA;            &#34;pqr789&#34;: &#34;&#34;,&#xA;            &#34;rst111&#34;: &#34;&#34;,&#xA;            &#34;mno222&#34;: &#34;&#34;,&#xA;            &#34;uvw333&#34;: &#34;&#34;&#xA;        };&#xA;&#xA;        // Get token from URL parameters&#xA;        const urlParams = new URLSearchParams(window.location.search);&#xA;        const token = urlParams.get(&#39;token&#39;);&#xA;&#xA;        if (token &amp;&amp; permanentLinks[token]) {&#xA;            const decodedUrl = atob(permanentLinks[token]); // Decode Base64 URL&#xA;            window.location.href = decodedUrl; // Redirect to original URL&#xA;        } else {&#xA;            document.body.innerHTML = &#34;&lt;h2&gt;Invalid or Expired Link!&lt;/h2&gt;&#34;;&#xA;        }&#xA;    &lt;/script&gt;&#xA;&lt;/body&gt;&#xA;&lt;/html&gt;</description>
    </item>
